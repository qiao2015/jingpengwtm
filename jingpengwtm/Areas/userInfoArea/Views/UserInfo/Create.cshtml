@model jingpengwtm.userInfoArea.ViewModels.UserInfoVMs.UserInfoVM
@inject IStringLocalizer<WalkingTec.Mvvm.Core.Program> Localizer;

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.ID" />
<wt:textbox field="Entity.PhoneNO" />
<wt:textbox field="Entity.Email" />
<wt:textbox field="Entity.PWD" />
<wt:textbox field="Entity.UserName" />
<wt:textbox field="Entity.ImagePath" />
<wt:textbox field="Entity.Signature" />
<wt:textbox field="Entity.ShowAll" />
<wt:datetime field="Entity.OptTime" />
<wt:textbox field="Entity.IsDel" />
<wt:textbox field="Entity.Age" />
<wt:textbox field="Entity.Address" />
<wt:textbox field="Entity.Sex" />
<wt:textbox field="Entity.LastToken" />
<wt:combobox field="Entity.IsShare" />
</wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
